{
  "name": "react-sequence-wrapper",
  "version": "0.1.0",
  "description": "a higher order component decorator providing state management for components that use a sequence of steps, like wizards and other multiple-step user interfaces.",
  "main": "lib/react-sequence-wrapper.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "npm run build:lib",
    "build:lib": "babel src --out-dir lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
  "type": "git",
  "url": "git+https://github.com/bitstrider/react-sequence-wrapper.git"
  },
  "keywords": [
    "react",
    "sequence",
    "wizard",
    "steps",
    "multi-step",
    "decorator",
    "hoc"
  ],
  "author": "Jason Yung",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.0.0",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^3.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "rimraf": "^2.6.1"
  }
}
